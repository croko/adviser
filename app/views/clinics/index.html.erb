<h1>Listing clinics</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Status</th>
      <th>User</th>
      <th>Published</th>
      <th>Reviews count</th>
      <th>Rating</th>
      <th>Likes count</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @clinics.each do |clinic| %>
      <tr>
        <td><%= clinic.name %></td>
        <td><%= clinic.description %></td>
        <td><%= clinic.status %></td>
        <td><%= clinic.user_id %></td>
        <td><%= clinic.published %></td>
        <td><%= clinic.reviews_count %></td>
        <td><%= clinic.rating %></td>
        <td><%= clinic.likes_count %></td>
        <td><%= link_to 'Show', clinic %></td>
        <td><%= link_to 'Edit', edit_clinic_path(clinic) %></td>
        <td><%= link_to 'Destroy', clinic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Clinic', new_clinic_path %>
